ot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">-- SELECT *
-- FROM stops
-- WHERE parent_station IS NOT NULL
-- ORDER BY stops.stop_name;

SELECT *
FROM stops
WHERE stop_id IN (
  SELECT DISTINCT parent_station
  FROM stops
  WHERE parent_station IS NOT NULL
)
ORDER BY stops.stop_name;</sql><sql name="SQL 2">SELECT *
FROM stops s
WHERE s.parent_station IS NOT NULL
   OR EXISTS (
       SELECT 1
       FROM stops c
       WHERE c.parent_station = s.stop_id
   )
ORDER BY s.parent_station;</sql><sql name="SQL 3">-- How many rows you'll keep
SELECT COUNT(*) AS to_keep
FROM stops s
WHERE s.parent_station IS NOT NULL
   OR EXISTS (SELECT 1 FROM stops c WHERE c.parent_station = s.stop_id);

-- How many rows you'll delete
SELECT COUNT(*) AS to_delete
FROM stops s
WHERE NOT (
  s.parent_station IS NOT NULL
  OR EXISTS (SELECT 1 FROM stops c WHERE c.parent_station = s.stop_id)
);</sql><sql name="SQL 4">DELETE FROM stops
WHERE parent_station IS NULL
  AND NOT EXISTS (
    SELECT 1
    FROM stops AS c
    WHERE c.parent_station = stops.stop_id
  );</sql><sql name="SQL 5">DELETE FROM stop_times AS st
WHERE NOT EXISTS (
  SELECT 1
  FROM stops AS s
  WHERE s.stop_id = st.stop_id
);</sql><sql name="SQL 6*">PRAGMA wal_checkpoint(FULL);
VACUUM;                -- not inside a transaction

</sql><sql name="SQL 7*">SELECT COUNT(*) AS orphan_count
FROM stop_times st
LEFT JOIN stops s ON s.stop_id = st.stop_id
WHERE s.stop_id IS NULL;</sql><current_tab id="6"/></tab_sql></sqlb_project>
